	section.tehnology
		.tehnology__inner 
			.tehnology__info
				.tehnology__container 
					.tehnology__body 
						.tehnology__content
							.heading-block
								h2.heading-block__title #[span современные] технологии
							.accordeon.tehnology-accordeon 
								each item, index in tehnology.accordeon
									.accordeon__item
										header.accordeon__header= item.header
