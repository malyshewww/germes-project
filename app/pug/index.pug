										.accordeon__content= item.content
			.tehnology__image-wrap
				.tehnology__image._ibg
					img(src="images/tehnology/img.jpg", alt="изображение")
	section.news 
		.container 
			.heading-block
				h2.heading-block__title новости
				.slider-controls 
					button(type="button").slider-button.slider-button-prev
					button(type="button").slider-button.slider-button-next
			.news-slider 
				.news-slider__body.swiper
					.news-slider__wrapper.swiper-wrapper
						each item in slider.news
							.news-slider__item.news-item.swiper-slide
								.news-item__body 
									.news-item__image._ibg
										img(src=`images/news/${item.img}.jpg`, alt="")
									.news-item__content 
										h3.news-item__title= item.title 
										.news-item__description
											p= item.descr 
										a(href="#").news-item__link= item.link 
											| #[span.news-item__icon]
	section.offers
		.container 
			.heading-block
				h2.heading-block__title Предложения от ведущих банков
				.heading-block__description Мы предоставляем наиболее выгодные предложения от ведущих банков
			.offers-slider
				.offers-slider__body.swiper
					.offers-slider__wrapper.swiper-wrapper
						each item in slider.offers
							.offers-slider__item.swiper-slide
								.offers-slider__image 
									img(src=`images/offers/${item.img}.png`, alt=item.name)
	section.dynamic
		.container 
			.heading-block
				h2.heading-block__title Динамика строительства
				.slider-controls 
					button(type="button").slider-button.slider-button-prev
					button(type="button").slider-button.slider-button-next
			.dynamic-slider
				.dynamic-slider__body.swiper
					.dynamic-slider__wrapper.swiper-wrapper
						each item, index in slider.dynamic
